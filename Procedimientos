DELIMITER $$
CREATE DEFINER=`root`@`localhost` PROCEDURE `insertarBaneo`(pFechaInicio varchar(11), pFechaFin varchar(11), pIdUsuario int)
BEGIN
INSERT INTO `Funes`.`Baneo`
(`FechaCreacion`,
`FechaDeFin`,
`idUsuario`)
VALUES
(pFechaInicio, 
pFechaFin,
pIdUsuario);

END$$
DELIMITER ;

