CREATE DEFINER=`root`@`localhost` PROCEDURE `insertUsuario`(pNombre varchar(25),
pPassword varchar(25),
pNick varchar(25),
pPrivado boolean)
BEGIN
INSERT INTO `Funes`.`Usuario`
(`Nombre`,
`Password`,
`Nick`,
`Privado`)
VALUES
(pNombre,
aes_encrypt(pPassword,1),
pNick,
pPrivado);
END$$
DELIMITER ;




